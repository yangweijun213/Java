<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>secondservlet</servlet-name>
		<servlet-class>com.bjsxt.servlet.SecondServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>secondservlet</servlet-name>
		<url-pattern>/s1</url-pattern>
		<url-pattern>/s2/*o</url-pattern>
		<url-pattern>/s2/a.do</url-pattern>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>lifecircle</servlet-name>
		<servlet-class>com.bjsxt.servlet.LifeCircleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>lifecircle</servlet-name>
		<url-pattern>/life</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>lifecircle2</servlet-name>
		<servlet-class>com.bjsxt.servlet.LifeCircleServlet</servlet-class>
<!-- 		<load-on-startup>1</load-on-startup> -->
	</servlet>
	<servlet-mapping>
		<servlet-name>lifecircle2</servlet-name>
		<url-pattern>/life2</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>dogetpost</servlet-name>
		<servlet-class>com.bjsxt.servlet.TestDogetDopost</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>dogetpost</servlet-name>
		<url-pattern>/getpost</url-pattern>
	</servlet-mapping>
</web-app>